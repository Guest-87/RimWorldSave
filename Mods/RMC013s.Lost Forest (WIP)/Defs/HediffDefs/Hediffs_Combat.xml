        <vomitMtbDays>0.25</vomitMtbDays>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.80</setMax>
          </li>
          <li>
            <capacity>Sight</capacity>
            <offset>-0.3</offset>
            <setMax>0.70</setMax>
          </li>
          <li>
            <capacity>Breathing</capacity>
            <offset>-0.5</offset>
            <setMax>0.50</setMax>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.5</offset>
            <setMax>0.50</setMax>
          </li>
        </capMods>
      </li>
      <li>
        <label>serious</label>
        <minSeverity>0.80</minSeverity>
        <painOffset>0.50</painOffset>
        <vomitMtbDays>0.25</vomitMtbDays>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.10</setMax>
          </li>
        </capMods>
        <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>Dementia</hediff>
            <mtbDays>10</mtbDays>
            <partsToAffect>
              <li>Brain</li>
            </partsToAffect>
          </li>
          <li Class="HediffGiver_Random">
            <hediff>Carcinoma</hediff>
            <mtbDays>30</mtbDays>
            <canAffectAnyLivePart>true</canAffectAnyLivePart>
          </li>
        </hediffGivers>
      </li>
    </stages>
  </HediffDef>

